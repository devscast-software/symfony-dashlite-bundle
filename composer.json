{
  "name": "devscast/symfony-dashlite-bundle",
  "type": "project",
  "license": "CC-BY-NC-SA-4.0",
  "description": "Integration of Dashlite theme in Symfony with Webpack Encore and Symfony UX",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3",
    "knplabs/knp-paginator-bundle": "^6.4",
    "symfony/config": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/form": "^7.1",
    "symfony/http-foundation": "^7.1",
    "symfony/http-kernel": "^7.1",
    "symfony/intl": "^7.1",
    "symfony/ux-twig-component": "^2.18",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.11.7",
    "phpunit/phpunit": "^9.6.20",
    "squizlabs/php_codesniffer": "^3.10.1",
    "symplify/easy-coding-standard": "^10.3.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Devscast\\Bundle\\DashliteBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Devscast\\Bundle\\DashliteBundle\\Tests\\": "tests/"
    }
  }
}
